{
	"name": "donatj/mddoc",
	"description": "Powerful, User Directed Markdown Documentation Generator",
	"require": {
		"php": ">=7.2",
		"ext-dom": "*",
		"ext-json": "*",
		"ext-libxml": "*",
		"donatj/cli-toolkit": "^0.3.1",
		"donatj/flags": "^1.5",
		"donatj/mddom": "^0.2.0",
		"phpdocumentor/reflection": "~5.2.0",
		"psr/log": "^1|^2|^3",
		"symfony/polyfill-php80": "^1.28"
	},
	"require-dev": {
		"corpus/coding-standard": "^0.8.0",
		"donatj/drop": "~1.0",
		"friendsofphp/php-cs-fixer": "^3.3",
		"php-parallel-lint/php-parallel-lint": "^1.3",
		"phpstan/phpstan": "^1.11",
		"squizlabs/php_codesniffer": "^3.7"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Jesse Donat",
			"email": "donatj@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"donatj\\MDDoc\\": "src/"
		}
	},
	"bin": [
		"composer/bin/mddoc"
	],
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		},
		"sort-packages": true
	}
}
